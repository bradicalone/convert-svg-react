{
  "name": "convert-svg-react",
  "version": "1.0.0",
  "description": "Converts SVG XML code to be used as ReactJS component.",
  "main": "lib/converter/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradicalone/convert-svg-react"
  },
  "keywords": [
    "Javascript",
    "React",
    "SVG",
    "JSX",
    "NodeJS",
    "svg"
  ],
  "scripts": {
    "compile": "npx babel src --watch --out-dir lib",
    "doc": "jsdoc -c jsdoc.json",
    "build:dev": "webpack --watch",
    "start:dev": "webpack serve --mode development --open --hot",
    "dev": "(npm run build:dev & npm run start:dev) || (start npm run build:dev && start npm run start:dev)",
    "test": "jest"
  },
  "author": "Brad Vanderbush <b.vanderbush@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "install": "^0.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.6.1",
    "jsdoc": "^3.6.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.3",
    "sass": "^1.28.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^1.2.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.7.4"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
